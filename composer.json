{
	"name": "trejjam/ares",
	"description": "Nette Ares",
	"keywords": [
		"nette",
		"ares",
		"ico"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Trejbal"
		}
	],
	"support": {
		"email": "jan.trejbal@tremi.cz",
		"issues": "https://gitlab.trem.in/trejjam/ares/issues"
	},
	"require": {
		"php": ">=7.1",

		"ext-simplexml": "^7.1",

		"nette/di": "~2.4",
		"nette/http": "~2.4",
		"nette/utils": "~2.4",
		"guzzlehttp/guzzle": "^6.2",
		"composer/ca-bundle": "^1.0",
		"trejjam/base-extension": "^0.8"
	},
	"require-dev": {
		"nette/tester": "^2.2",

		"phpstan/phpstan": "^0.11",
		"phpstan/phpstan-deprecation-rules": "^0.11",
		"phpstan/phpstan-strict-rules": "^0.11",
		"thecodingmachine/phpstan-safe-rule": "^0.1",
		"roave/no-floaters": "dev-master",

		"symplify/easy-coding-standard": "^5.0",

		"sensiolabs/security-checker": "^5.0",
		"roave/security-advisories": "dev-master"
	},
	"suggest": {

	},
	"autoload": {
		"psr-4": {
			"Trejjam\\Ares\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Trejjam\\Ares\\Tests\\": "tests/src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "v0.2.x-dev"
		}
	}
}
